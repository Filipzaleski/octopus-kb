    "@babel/eslint-parser": "^7.0.0",
    "babel-jest": "^26.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-react-app": "^10.0.0",
    "babel-runtime": "^7.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^4.0.0",
    "css-loader": "^5.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.0.0",
    "eslint-config-react-app": "^5.0.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
